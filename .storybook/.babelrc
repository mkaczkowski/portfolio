{
  "presets": [
    "react-app"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@core": "./src/core",
        "@components": "./src/components",
        "@assets": "./src/assets",
        "@theme": "./src/theme",
        "@lib": "./src/lib",
        "@story": "./.storybook",
        "@products": "./products",
        "@root": "./src"
      }
    }]
  ]
}
