{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "serve": "serve -s build",
    "build:source-map": "cross-env GENERATE_SOURCEMAP=true node scripts/build.js",
    "build": "cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "postbuild": "react-snap",
    "test": "node scripts/test.js --env=jsdom",
    "precommit": "lint-staged",
    "flow": "flow",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.4",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.18",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.67.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "loadable-components": "^1.1.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.1.1",
    "postcss-reporter": "^5.0.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prettier": "^1.11.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-snap": "^1.11.4",
    "recompose": "^0.26.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "preloadImages": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addon-viewport": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "i18n-webpack-plugin": "^1.0.0",
    "image-webpack-loader": "^4.1.0",
    "postcss-import": "^11.1.0",
    "react-svg-loader": "^2.1.0",
    "storybook-host": "^4.1.5",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-flush-chunks-html": "^1.0.2"
  }
